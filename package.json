{
  "name": "slpg",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "postbuild": "next-on-netlify",
    "dev": "next dev",
    "eslint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx",
    "gen": "yarn graphql:codegen",
    "graphql:codegen": "graphql-codegen --config graphql-codegen.js",
    "lint": "yarn eslint src",
    "local-tunnel": "lt -p 3000 -s slpg-proxy-local",
    "netlify-build": "yarn build && yarn site-map",
    "qa": "run-p type-check test lint",
    "site-map": "node site-map.js",
    "start": "next start",
    "test": "echo 'no tests yet'",
    "type-check": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged",
      "pre-push": "yarn qa"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },

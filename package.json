{
  "name": "slpg",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm-run-all --parallel build:app build:lambda",
    "build:app": "next build",
    "build:lambda": "netlify-lambda build --config ./webpack.functions.js src/lambda",
    "dev": "npm-run-all --parallel dev:app dev:lambda",
    "dev:app": "node server.js",
    "dev:lambda": "netlify-lambda serve --config ./webpack.functions.js src/lambda",
    "eslint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx",
    "export": "next export",
    "gen": "yarn graphql:codegen",
    "graphql:codegen": "graphql-codegen --config graphql-codegen.js",
    "lint": "yarn eslint src",
    "netlify-build": "yarn build && yarn export && yarn site-map",
    "qa": "run-p type-check test lint",
    "site-map": "node site-map.js",
    "start": "next start",
    "test": "jest",
    "type-check": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged",
      "pre-push": "yarn qa"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    }
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "apollo-boost": "^0.4.4",
    "apollo-link-schema": "^1.2.4",
    "babel-plugin-macros": "^2.6.2",
    "classnames": "^2.2.6",
    "console-suppress": "^0.1.1",
    "date-fns": "^2.10.0",
    "dayjs": "^1.8.17",
    "formik": "^2.0.6",
    "googleapis": "^45.0.0",
    "graphql": "^14.5.8",
    "graphql-playground-react": "^1.7.20",
    "graphql-type-json": "^0.3.1",
    "js-yaml-loader": "^1.2.2",
    "keycode": "^2.2.0",
    "mailchimp-api-v3": "^1.13.1",
    "md5": "^2.2.1",
    "netlify-lambda": "^1.6.3",
    "next": "9.1.3",
    "nodemailer": "^6.4.1",
    "querystring": "^0.2.0",
    "ramda": "^0.26.1",
    "react": "16.12.0",
    "react-alert": "^6.0.0",
    "react-alert-template-basic": "^1.0.0",
    "react-burgers": "^1.3.0",
    "react-dom": "16.12.0",
    "react-markdown": "^4.2.2",
    "react-no-ssr": "^1.1.0",
    "react-redux": "^5.1.2",
    "react-spinners": "^0.6.1",
    "react-spring": "^8.0.27",
    "sitemap": "^5.1.0",
    "styled-components": "4.3.2",
    "styled-tools": "^1.7.1",
    "tailwind.macro": "^1.0.0-alpha.10",
    "tailwindcss": "^1.1.3",
    "tinycolor2": "^1.4.1",
    "url-parse": "^1.4.7",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.8.3",
    "@graphql-codegen/fragment-matcher": "^1.8.3",
    "@graphql-codegen/schema-ast": "^1.8.3",
    "@graphql-codegen/typescript": "^1.8.3",
    "@graphql-codegen/typescript-operations": "^1.8.3",
    "@graphql-codegen/typescript-resolvers": "^1.9.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.7",
    "@types/nodemailer": "^6.2.2",
    "@types/react": "^16.9.11",
    "@types/react-alert": "^5.2.0",
    "@types/styled-components": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "@zeit/next-css": "^1.0.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.6",
    "dotenv-load": "^2.0.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "express": "^4.17.1",
    "frontmatter-markdown-loader": "^3.1.0",
    "glob": "^7.1.6",
    "graphql-request": "^1.8.2",
    "graphql-tag": "^2.10.1",
    "http-proxy-middleware": "^0.20.0",
    "husky": "^3.0.9",
    "jest": "^25.1.0",
    "lint-staged": "^9.4.3",
    "next-compose-plugins": "^2.2.0",
    "next-env": "^1.1.0",
    "next-images": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "shell-quote": "^1.7.2",
    "sort-package-json": "^1.23.1",
    "typescript": "^3.7.2"
  },
  "resolutions": {
    "hoist-non-react-statics": "3.0.0"
  }
}

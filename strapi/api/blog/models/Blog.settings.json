{
  "connection": "default",
  "collectionName": "blogs",
  "info": {
    "name": "blog",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "title": {
      "required": true,
      "type": "string",
      "unique": true
    },
    "slug": {
      "required": true,
      "type": "string",
      "unique": true,
      "default": "Preenchido automaticamente"
    },
    "body": {
      "required": true,
      "type": "richtext"
    },
    "summary": {
      "type": "text"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    }
  }
}